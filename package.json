    "start": "env-cmd -e prod -r .env.json node server/index.js"
  },
  "dependencies": {
    "@nuxt/content": "^1.14.0",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/gtm": "^2.4.0",
    "@nuxtjs/i18n": "^7.2.0",
    "@nuxtjs/pwa": "^3.3.5",
    "core-js": "^3.15.1",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "nuxt": "^2.15.7",
    "vee-validate": "^3.4.14",
    "vue-persian-datetime-picker": "^2.10.1"
  },
  "devDependencies": {
    "@nuxtjs/google-analytics": "^2.4.0",
    "@nuxtjs/router": "^1.6.1",
    "@nuxtjs/style-resources": "^1.2.1",
    "eslint-config-prettier": "^8.3.0",
    "node-sass": "^5.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.3.2",
    "sass-loader": "^10.2.0"
  }
